name: otconda@PY_MAJOR_VER@
version: 1.11rc1

ignore_duplicate_files: True
install_in_dependency_order: True

channels:
  - https://conda.anaconda.org/conda-forge/
  - https://repo.continuum.io/pkgs/free/
  - https://repo.continuum.io/pkgs/main/
  - https://repo.continuum.io/pkgs/r/
  - https://repo.continuum.io/pkgs/pro/
  - https://repo.continuum.io/pkgs/msys2/  [win]

specs:
  - python @PY_MAJOR_VER@.*
  - conda
  - console_shortcut  [win]
  - menuinst  [win]
  - pytz 2017.3  # noarch
  - python-dateutil 2.6.1  # noarch
  - numpy >=1.14  [win]
  - scikit-learn
  - pandas
  - matplotlib
  - otfftw
  - otfmi
  - otlm
  - otmixmod
  - otmorris
  - otpmml
  #- otpod
  - otrobopt
  - otsvm
  - otwrapy

license_file: EULA.txt

welcome_image: galton_v.png
header_image: galton_h.png
icon_image: icon256.png
